@model IEnumerable<DSWebUI.Models.StoreLocationVM>

@{
    ViewData["Title"] = "Index";
}

<h1>@ViewBag.DogManager.Name</h1>

<p>
    @Html.ActionLink("Add a New Store", "Create", new { id = ViewBag.DogManager.PhoneNumber })
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Location)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                @Html.ActionLink("Order History", "StoHisCreate", "OrderHistory", new {id = item.Id}) |
                <!--@Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ })-->
                @Html.ActionLink("Items", "Index", "Inventory", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id, num = ViewBag.DogManager.PhoneNumber })
            </td>
        </tr>
}
    </tbody>
</table>
